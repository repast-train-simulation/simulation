<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/>
				<parameter name="train_capacity" displayName="Capacidad de cada vagon" type="int" 
						defaultValue="100" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
											/><parameter
					name="train_velocity_kmh" displayName="Velocidad del tren en km/h"
					type="int" isReadOnly="false"
					converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
					defaultValue="59">
</parameter>
	<parameter name="train_stop_time_minutes"
		displayName="Tiempo de parada del tren en minutos" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
		type="int" defaultValue="2">
	</parameter>
	<parameter name="number_of_trains" defaultValue="1"
		displayName="Cantidad de trenes" isReadOnly="false" type="int"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter">
	</parameter>
	<parameter name="train_interval"
		displayName="Intervalo de tiempo entre trenes"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
		defaultValue="1" isReadOnly="false" type="int">
	</parameter>
</parameters>


